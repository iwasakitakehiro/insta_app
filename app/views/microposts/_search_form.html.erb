<%= form_with url: search_microposts_path, method: :get, local: true do |f| %>
  <div class="form-inline my-2 my-lg-0">
    <div class= "field mr-sm-2">
    <%= f.text_field :content,placeholder: "画像を検索"%>
    </div>
    <div class="field my-2 my-sm-0">
    <%= f.submit :search %>
    </div>
  </div>
<% end %>